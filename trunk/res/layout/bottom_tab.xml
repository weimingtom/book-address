<?xml version="1.0" encoding="utf-8"?>
<TableLayout android:layout_width="fill_parent"
		android:layout_height="54dip"
		android:orientation="horizontal"
		android:layout_gravity="bottom"
		android:background="@drawable/bg_tab_pressed"
		android:layout_alignParentBottom="true"
		xmlns:android="http://schemas.android.com/apk/res/android"
		>
		<TableRow
			android:layout_width="fill_parent"
		    android:layout_height="54dip"
			>
			<ImageButton 
				android:id="@+id/last_contact"
				android:layout_width="70dip"
				android:layout_height="54dip"
				android:src="@drawable/bg_tab_dial_normal"
				android:background="@drawable/bottom_bg"
				android:gravity="center"
				android:layout_weight="1"
				/>
			<ImageButton 
			    android:id="@+id/contact_people"
				android:layout_width="70dip"
				android:layout_height="54dip"
				android:background="@drawable/bottom_bg"
				android:src="@drawable/bg_tab_contact_normal"
				android:gravity="center"
				android:layout_weight="1"
				/>
	        <ImageButton 
	            android:id="@+id/sms_message"
				android:layout_width="70dip"
				android:layout_height="54dip"
				android:background="@drawable/bottom_bg"
				android:src="@drawable/bg_tab_sms_normal"
				android:gravity="center"
				android:layout_weight="1"
				/>
			<ImageButton 
			    android:id="@+id/setting"
				android:layout_width="70dip"
				android:layout_height="54dip"
				android:background="@drawable/bottom_bg"
				android:src="@drawable/bg_tab_setting_normal"
				android:gravity="center"
				android:layout_weight="1"
				/>		
		</TableRow>		
	</TableLayout>